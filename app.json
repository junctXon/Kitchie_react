{
  "expo": {
    "name": "Kitchie",
    "icon": "./assets/images/app_icon.png",
    "slug": "Kitchie",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "kitchie",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "backgroundColor": "#f7b7a3",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.onoojo.kitchie",
      "backgroundColor": "#f7b7a3",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.onoojo.kitchie",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app_icon.png",
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#f7b7a3"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2fad1beb-542b-49d7-b2a7-56eb24fce8d0"
      }
    }
  }
}